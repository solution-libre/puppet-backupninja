    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],

  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 2.3.0 < 5.0.0" }
  ]
}
